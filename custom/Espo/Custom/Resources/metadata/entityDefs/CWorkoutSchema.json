{
    "fields": {
        "name": {
            "type": "varchar",
            "required": true,
            "pattern": "$noBadCharacters"
        },
        "description": {
            "type": "text"
        },
        "createdAt": {
            "type": "datetime",
            "readOnly": true
        },
        "modifiedAt": {
            "type": "datetime",
            "readOnly": true
        },
        "createdBy": {
            "type": "link",
            "readOnly": true,
            "view": "views/fields/user"
        },
        "modifiedBy": {
            "type": "link",
            "readOnly": true,
            "view": "views/fields/user"
        },
        "assignedUser": {
            "type": "link",
            "required": false,
            "view": "views/fields/assigned-user"
        },
        "teams": {
            "type": "linkMultiple",
            "view": "views/fields/teams"
        },
        "schemaType": {
            "type": "enum",
            "required": true,
            "options": [
                "Core",
                "Cardio - Core",
                "Kracht - Core",
                "Kracht - Cardio - Core"
            ],
            "default": "Cardio - Core",
            "style": {
                "Core": null,
                "Cardio - Core": null,
                "Kracht - Core": null,
                "Kracht - Cardio - Core": null
            },
            "maxLength": 100,
            "isCustom": true
        },
        "contact": {
            "type": "link"
        },
        "workoutSessies": {
            "type": "linkMultiple"
        }
    },
    "links": {
        "createdBy": {
            "type": "belongsTo",
            "entity": "User"
        },
        "modifiedBy": {
            "type": "belongsTo",
            "entity": "User"
        },
        "assignedUser": {
            "type": "belongsTo",
            "entity": "User"
        },
        "teams": {
            "type": "hasMany",
            "entity": "Team",
            "relationName": "entityTeam",
            "layoutRelationshipsDisabled": true
        },
        "contact": {
            "type": "belongsTo",
            "foreign": "cWorkoutSchemas",
            "entity": "Contact",
            "audited": true,
            "isCustom": true
        },
        "workoutSessies": {
            "type": "hasMany",
            "foreign": "workoutSchema",
            "entity": "CWorkoutSessie",
            "audited": true,
            "isCustom": true
        }
    },
    "collection": {
        "orderBy": "createdAt",
        "order": "desc"
    },
    "indexes": {
        "name": {
            "columns": [
                "name",
                "deleted"
            ]
        },
        "assignedUser": {
            "columns": [
                "assignedUserId",
                "deleted"
            ]
        },
        "createdAt": {
            "columns": [
                "createdAt"
            ]
        },
        "createdAtId": {
            "unique": true,
            "columns": [
                "createdAt",
                "id"
            ]
        }
    }
}