{
    "fields": {
        "name": {
            "type": "varchar",
            "required": true,
            "pattern": "$noBadCharacters"
        },
        "description": {
            "type": "text"
        },
        "createdAt": {
            "type": "datetime",
            "readOnly": true
        },
        "modifiedAt": {
            "type": "datetime",
            "readOnly": true
        },
        "createdBy": {
            "type": "link",
            "readOnly": true,
            "view": "views/fields/user"
        },
        "modifiedBy": {
            "type": "link",
            "readOnly": true,
            "view": "views/fields/user"
        },
        "assignedUser": {
            "type": "link",
            "required": false,
            "view": "views/fields/assigned-user"
        },
        "teams": {
            "type": "linkMultiple",
            "view": "views/fields/teams"
        },
        "roundRobinActief": {
            "notNull": true,
            "type": "bool",
            "isCustom": true
        },
        "roundRobinStart": {
            "notNull": false,
            "type": "date",
            "default": "javascript: return this.dateTime.getToday();",
            "isCustom": true,
            "required": true
        },
        "activeMembers": {
            "type": "linkMultiple"
        },
        "slimFitCenter": {
            "type": "link",
            "required": true,
            "audited": true
        },
        "campaign": {
            "type": "link",
            "required": true,
            "audited": true
        },
        "leads": {
            "type": "linkMultiple"
        }
    },
    "links": {
        "createdBy": {
            "type": "belongsTo",
            "entity": "User"
        },
        "modifiedBy": {
            "type": "belongsTo",
            "entity": "User"
        },
        "assignedUser": {
            "type": "belongsTo",
            "entity": "User"
        },
        "teams": {
            "type": "hasMany",
            "entity": "Team",
            "relationName": "entityTeam",
            "layoutRelationshipsDisabled": true
        },
        "activeMembers": {
            "type": "hasMany",
            "relationName": "cActiveCampaignsRoundRobinMembers",
            "foreign": "activeCampaigns",
            "entity": "CTeam",
            "audited": false,
            "isCustom": true
        },
        "slimFitCenter": {
            "type": "belongsTo",
            "foreign": "campagneRoundRobins",
            "entity": "CSlimFitCenter",
            "audited": false,
            "isCustom": true
        },
        "campaign": {
            "type": "belongsTo",
            "foreign": "cCampagneRoundRobins",
            "entity": "Campaign",
            "audited": false,
            "isCustom": true
        },
        "leads": {
            "type": "hasMany",
            "foreign": "cCampagneRoundRobin",
            "entity": "Lead",
            "audited": false,
            "isCustom": true
        }
    },
    "collection": {
        "orderBy": "createdAt",
        "order": "desc",
        "textFilterFields": [
            "name"
        ],
        "fullTextSearch": false,
        "countDisabled": false
    },
    "indexes": {
        "name": {
            "columns": [
                "name",
                "deleted"
            ]
        },
        "assignedUser": {
            "columns": [
                "assignedUserId",
                "deleted"
            ]
        },
        "createdAt": {
            "columns": [
                "createdAt"
            ]
        },
        "createdAtId": {
            "unique": true,
            "columns": [
                "createdAt",
                "id"
            ]
        }
    },
    "optimisticConcurrencyControl": false
}