{
    "fields": {
        "name": {
            "type": "varchar",
            "required": true,
            "pattern": "$noBadCharacters",
            "options": []
        },
        "description": {
            "type": "text"
        },
        "createdAt": {
            "type": "datetime",
            "readOnly": true
        },
        "modifiedAt": {
            "type": "datetime",
            "readOnly": true
        },
        "createdBy": {
            "type": "link",
            "readOnly": true,
            "view": "views/fields/user"
        },
        "modifiedBy": {
            "type": "link",
            "readOnly": true,
            "view": "views/fields/user"
        },
        "assignedUser": {
            "type": "link",
            "required": false,
            "view": "views/fields/assigned-user"
        },
        "teams": {
            "type": "linkMultiple",
            "view": "views/fields/teams"
        },
        "gewicht": {
            "type": "int",
            "isCustom": true,
            "min": 0,
            "max": 250
        },
        "aantal": {
            "type": "int",
            "isCustom": true,
            "min": 1,
            "max": 50
        },
        "reeksen": {
            "type": "int",
            "isCustom": true,
            "min": 1,
            "max": 5
        },
        "type": {
            "type": "enum",
            "required": true,
            "options": [
                "Cardio",
                "Core",
                "Kracht"
            ],
            "default": "Cardio",
            "isSorted": true,
            "displayAsLabel": true,
            "audited": true,
            "style": {
                "Cardio": null,
                "Core": null,
                "Kracht": null
            },
            "maxLength": 100,
            "isCustom": true
        },
        "subType": {
            "type": "enum",
            "required": true,
            "options": [
                "Opwarming",
                "Basis Uithouding",
                "Interval",
                "Cooldown",
                "Armen",
                "Borst",
                "Schouders",
                "Rug",
                "Statisch",
                "Dynamisch",
                "Rotatie / Anti-rotatie",
                "Stability / Balance"
            ],
            "default": "Opwarming",
            "isSorted": true,
            "displayAsLabel": true,
            "style": {
                "Opwarming": null,
                "Basis Uithouding": null,
                "Interval": null,
                "Cooldown": null,
                "Armen": null,
                "Borst": null,
                "Schouders": null,
                "Rug": null,
                "Statisch": null,
                "Dynamisch": null,
                "Rotatie / Anti-rotatie": null,
                "Stability / Balance": null
            },
            "maxLength": 100,
            "isCustom": true
        },
        "minHartslag": {
            "type": "int",
            "min": 45,
            "max": 220,
            "isCustom": true
        },
        "maxHartslag": {
            "type": "int",
            "min": 45,
            "max": 220,
            "isCustom": true
        },
        "workoutSessie": {
            "type": "link",
            "required": true
        }
    },
    "links": {
        "createdBy": {
            "type": "belongsTo",
            "entity": "User"
        },
        "modifiedBy": {
            "type": "belongsTo",
            "entity": "User"
        },
        "assignedUser": {
            "type": "belongsTo",
            "entity": "User"
        },
        "teams": {
            "type": "hasMany",
            "entity": "Team",
            "relationName": "entityTeam",
            "layoutRelationshipsDisabled": true
        },
        "workoutSessie": {
            "type": "belongsTo",
            "foreign": "workoutOefening",
            "entity": "CWorkoutSessie",
            "audited": false,
            "isCustom": true
        }
    },
    "collection": {
        "orderBy": "createdAt",
        "order": "desc"
    },
    "indexes": {
        "name": {
            "columns": [
                "name",
                "deleted"
            ]
        },
        "assignedUser": {
            "columns": [
                "assignedUserId",
                "deleted"
            ]
        },
        "createdAt": {
            "columns": [
                "createdAt"
            ]
        },
        "createdAtId": {
            "unique": true,
            "columns": [
                "createdAt",
                "id"
            ]
        }
    }
}